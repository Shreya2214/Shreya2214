# ğŸš¨ Don't change the code below ğŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ğŸš¨ Don't change the code above ğŸ‘†

#Write your code below this line ğŸ‘‡
x = name1.lower()
y = name2.lower()
 
m = x.count("t") + x.count("r") + x.count("u") + x.count("e") + y.count("t") + y.count("r") + y.count("u") + y.count("e")
n = x.count("l") + x.count("o") + x.count("v") + x.count("e") + y.count("l") + y.count("o") + y.count("v") + y.count("e")

Percentage = str(m) + str(n)
Percentage = int(Percentage)

if (Percentage < 10) or (Percentage > 90):
    print(f"Your score is {Percentage}, you go together like coke and mentos.")
elif (Percentage > 40) and (Percentage < 50):
    print((f"Your score is {Percentage}, you are alright together."))
else:
    print(f"Your score is {Percentage}.")
